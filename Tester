import java.text.SimpleDateFormat;
import java.util.Date;
public class Tester extends Employee{
    private int solvedBugs;


    public Tester(String name, int birthDate, double monthlyIncome, int occupationRate, int solvedBugs) {
        super(name, birthDate, monthlyIncome, occupationRate);
        this.solvedBugs = solvedBugs;
    }

    public int getSolvedBugs() {
        return solvedBugs;
    }

    public void setSolvedBugs(int solvedBugs) {
        this.solvedBugs = solvedBugs;
    }
}
